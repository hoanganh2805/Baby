<div id="<%= dom_id customer %>">
  <p>
    <strong>Name:</strong>
    <%= customer.name %>
  </p>

  <p>
    <strong>Id:</strong>
    <%= customer.ID %>
  </p>

  <p>
    <strong>Dateofbirth:</strong>
    <%= customer.DateofBirth %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= customer.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= customer.email %>
  </p>

  <p>
    <strong>Level:</strong>
    <%= customer.level %>
  </p>

</div>
