<div id="<%= dom_id menu %>">
  <p>
    <strong>Dishes:</strong>
    <%= menu.dishes %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= menu.price %>
  </p>

  <p>
    <strong>Ingredients:</strong>
    <%= menu.ingredients %>
  </p>

  <p>
    <strong>Supplier:</strong>
    <%= menu.supplier_id %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= menu.category_id %>
  </p>

  <p>
    <strong>Discription:</strong>
    <%= menu.discription %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= menu.status %>
  </p>

</div>
